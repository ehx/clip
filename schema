CREATE TABLE "django_migrations" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "app" varchar(255) NOT NULL, "name" varchar(255) NOT NULL, "applied" datetime NOT NULL);
CREATE TABLE "corsheaders_corsmodel" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "cors" varchar(255) NOT NULL);
CREATE TABLE "auth_group" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "name" varchar(80) NOT NULL UNIQUE);
CREATE TABLE "auth_group_permissions" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "group_id" integer NOT NULL REFERENCES "auth_group" ("id"), "permission_id" integer NOT NULL REFERENCES "auth_permission" ("id"), UNIQUE ("group_id", "permission_id"));
CREATE TABLE "auth_user_groups" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "user_id" integer NOT NULL REFERENCES "auth_user" ("id"), "group_id" integer NOT NULL REFERENCES "auth_group" ("id"), UNIQUE ("user_id", "group_id"));
CREATE TABLE "auth_user_user_permissions" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "user_id" integer NOT NULL REFERENCES "auth_user" ("id"), "permission_id" integer NOT NULL REFERENCES "auth_permission" ("id"), UNIQUE ("user_id", "permission_id"));
CREATE INDEX "auth_group_permissions_0e939a4f" ON "auth_group_permissions" ("group_id");
CREATE INDEX "auth_group_permissions_8373b171" ON "auth_group_permissions" ("permission_id");
CREATE INDEX "auth_user_groups_e8701ad4" ON "auth_user_groups" ("user_id");
CREATE INDEX "auth_user_groups_0e939a4f" ON "auth_user_groups" ("group_id");
CREATE INDEX "auth_user_user_permissions_e8701ad4" ON "auth_user_user_permissions" ("user_id");
CREATE INDEX "auth_user_user_permissions_8373b171" ON "auth_user_user_permissions" ("permission_id");
CREATE TABLE "django_admin_log" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "action_time" datetime NOT NULL, "object_id" text NULL, "object_repr" varchar(200) NOT NULL, "action_flag" smallint unsigned NOT NULL, "change_message" text NOT NULL, "content_type_id" integer NULL REFERENCES "django_content_type" ("id"), "user_id" integer NOT NULL REFERENCES "auth_user" ("id"));
CREATE INDEX "django_admin_log_417f1b1c" ON "django_admin_log" ("content_type_id");
CREATE INDEX "django_admin_log_e8701ad4" ON "django_admin_log" ("user_id");
CREATE TABLE "django_content_type" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "app_label" varchar(100) NOT NULL, "model" varchar(100) NOT NULL, UNIQUE ("app_label", "model"));
CREATE TABLE "auth_permission" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "content_type_id" integer NOT NULL REFERENCES "django_content_type" ("id"), "codename" varchar(100) NOT NULL, "name" varchar(255) NOT NULL, UNIQUE ("content_type_id", "codename"));
CREATE INDEX "auth_permission_417f1b1c" ON "auth_permission" ("content_type_id");
CREATE TABLE "auth_user" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "password" varchar(128) NOT NULL, "is_superuser" bool NOT NULL, "username" varchar(30) NOT NULL UNIQUE, "first_name" varchar(30) NOT NULL, "last_name" varchar(30) NOT NULL, "email" varchar(254) NOT NULL, "is_staff" bool NOT NULL, "is_active" bool NOT NULL, "date_joined" datetime NOT NULL, "last_login" datetime NULL);
CREATE TABLE "authtoken_token" ("key" varchar(40) NOT NULL PRIMARY KEY, "created" datetime NOT NULL, "user_id" integer NOT NULL UNIQUE REFERENCES "auth_user" ("id"));
CREATE TABLE "django_session" ("session_key" varchar(40) NOT NULL PRIMARY KEY, "session_data" text NOT NULL, "expire_date" datetime NOT NULL);
CREATE INDEX "django_session_de54fa62" ON "django_session" ("expire_date");
CREATE TABLE "task_todo" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "description" varchar(255) NOT NULL, "done" bool NOT NULL, "created_at" datetime NOT NULL, "task_id" integer NULL REFERENCES "task_task" ("id"), "user_id" integer NOT NULL REFERENCES "auth_user" ("id"), "updated_at" datetime NOT NULL);
CREATE INDEX "task_todo_57746cc8" ON "task_todo" ("task_id");
CREATE INDEX "task_todo_e8701ad4" ON "task_todo" ("user_id");
CREATE TABLE "task_client" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "name" varchar(50) NOT NULL, "lastname" varchar(50) NOT NULL, "address" varchar(50) NOT NULL, "telephone" integer NOT NULL, "mail" varchar(254) NOT NULL, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
CREATE TABLE "task_message" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "message" varchar(255) NOT NULL, "read" bool NOT NULL, "created_at" datetime NOT NULL, "owner_id" integer NOT NULL UNIQUE REFERENCES "auth_user" ("id"), "receptor_id" integer NOT NULL UNIQUE REFERENCES "auth_user" ("id"), "updated_at" datetime NOT NULL);
CREATE TABLE "task_module" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "tag" varchar(50) NOT NULL, "name" varchar(50) NOT NULL, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
CREATE TABLE "task_organization" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "name" varchar(50) NOT NULL, "address" varchar(50) NOT NULL, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
CREATE TABLE "task_status" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "name" varchar(50) NOT NULL, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
CREATE TABLE "task_urgency" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "name" varchar(50) NOT NULL, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
CREATE TABLE "task_userclient" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "relation" varchar(1) NOT NULL, "user_id" integer NOT NULL REFERENCES "auth_user" ("id"), "userR_id" integer NOT NULL REFERENCES "auth_user" ("id"), "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL, UNIQUE ("user_id", "userR_id"));
CREATE INDEX "task_userclient_e8701ad4" ON "task_userclient" ("user_id");
CREATE INDEX "task_userclient_107949c0" ON "task_userclient" ("userR_id");
CREATE TABLE "task_userprofile" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "online" bool NOT NULL, "organization_id" integer NOT NULL REFERENCES "task_organization" ("id"), "user_id" integer NOT NULL UNIQUE REFERENCES "auth_user" ("id"), "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL);
CREATE INDEX "task_userprofile_26b2345e" ON "task_userprofile" ("organization_id");
CREATE TABLE "task_task" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "title" varchar(100) NOT NULL, "priority" integer NULL, "start_date" date NULL, "finish_date" date NULL, "estimation_hours" integer NOT NULL, "description" varchar(255) NOT NULL, "identificator" integer NOT NULL, "done" bool NOT NULL, "client_id" integer NULL REFERENCES "auth_user" ("id"), "module_id" integer NULL REFERENCES "task_module" ("id"), "status_id" integer NULL REFERENCES "task_status" ("id"), "urgency_id" integer NULL REFERENCES "task_urgency" ("id"), "user_id" integer NULL REFERENCES "auth_user" ("id"), "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL, "implementation_id" varchar(50) NOT NULL);
CREATE INDEX "task_task_2bfe9d72" ON "task_task" ("client_id");
CREATE INDEX "task_task_c9799665" ON "task_task" ("module_id");
CREATE INDEX "task_task_dc91ed4b" ON "task_task" ("status_id");
CREATE INDEX "task_task_a9707a99" ON "task_task" ("urgency_id");
CREATE INDEX "task_task_e8701ad4" ON "task_task" ("user_id");
CREATE TABLE "task_taskcomment" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "comment" varchar(5000) NOT NULL, "created_at" datetime NOT NULL, "task_id" integer NOT NULL REFERENCES "task_task" ("id"), "user_id" integer NOT NULL REFERENCES "auth_user" ("id"), "updated_at" datetime NOT NULL, "docfile" varchar(100) NULL);
CREATE INDEX "task_taskcomment_57746cc8" ON "task_taskcomment" ("task_id");
CREATE INDEX "task_taskcomment_e8701ad4" ON "task_taskcomment" ("user_id");
CREATE TABLE "task_notification" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "ntype" varchar(50) NOT NULL, "task" integer NOT NULL, "read" bool NOT NULL, "user_id" integer NOT NULL REFERENCES "auth_user" ("id"), "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL, "userN_id" integer NOT NULL REFERENCES "auth_user" ("id"));
CREATE INDEX "task_notification_e8701ad4" ON "task_notification" ("user_id");
CREATE INDEX "task_notification_12234cc7" ON "task_notification" ("userN_id");
CREATE TABLE "task_milestone" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "date" date NOT NULL, "description" varchar(255) NOT NULL, "created_at" datetime NOT NULL, "updated_at" datetime NOT NULL, "task_id" integer NULL REFERENCES "task_task" ("id"), "user_id" integer NOT NULL REFERENCES "auth_user" ("id"));
CREATE INDEX "task_milestone_57746cc8" ON "task_milestone" ("task_id");
CREATE INDEX "task_milestone_e8701ad4" ON "task_milestone" ("user_id");
